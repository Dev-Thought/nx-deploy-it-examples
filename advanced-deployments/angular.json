{
  "version": 1,
  "projects": {
    "i-love-iac": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/i-love-iac",
      "sourceRoot": "apps/i-love-iac/src",
      "prefix": "advanced-deployments",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/i-love-iac",
            "index": "apps/i-love-iac/src/index.html",
            "main": "apps/i-love-iac/src/main.ts",
            "polyfills": "apps/i-love-iac/src/polyfills.ts",
            "tsConfig": "apps/i-love-iac/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/i-love-iac/src/favicon.ico",
              "apps/i-love-iac/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/i-love-iac/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/i-love-iac/src/environments/environment.ts",
                  "with": "apps/i-love-iac/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "i-love-iac:build",
            "proxyConfig": "apps/i-love-iac/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "i-love-iac:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "i-love-iac:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/i-love-iac/tsconfig.app.json",
              "apps/i-love-iac/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/i-love-iac/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/i-love-iac/jest.config.js",
            "tsConfig": "apps/i-love-iac/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/i-love-iac/src/test-setup.ts"
          }
        },
        "deploy": {
          "builder": "@dev-thought/nx-deploy-it:deploy",
          "options": {
            "main": "apps/i-love-iac/infrastructure/index.ts",
            "provider": "azure",
            "pulumi": {
              "azure:location": "germanywestcentral",
              "useCdn": false
            }
          },
          "configurations": {
            "production": {
              "pulumi": {
                "useCdn": true
              }
            }
          }
        },
        "destroy": {
          "builder": "@dev-thought/nx-deploy-it:destroy",
          "options": {
            "main": "apps/i-love-iac/infrastructure/index.ts",
            "provider": "azure"
          }
        }
      }
    },
    "i-love-iac-e2e": {
      "root": "apps/i-love-iac-e2e",
      "sourceRoot": "apps/i-love-iac-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/i-love-iac-e2e/cypress.json",
            "tsConfig": "apps/i-love-iac-e2e/tsconfig.e2e.json",
            "devServerTarget": "i-love-iac:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "i-love-iac:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/i-love-iac-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/i-love-iac-e2e/**"
            ]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/api/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@dev-thought/nx-deploy-it:deploy",
          "options": {
            "main": "apps/api/infrastructure/index.ts",
            "provider": "azure",
            "pulumi": {
              "azure:location": "germanywestcentral"
            }
          }
        },
        "destroy": {
          "builder": "@dev-thought/nx-deploy-it:destroy",
          "options": {
            "main": "apps/api/infrastructure/index.ts",
            "provider": "azure"
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/api-interfaces/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "tsConfig": "libs/api-interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "i-love-iac"
}